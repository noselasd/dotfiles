# Source this file to enable virtualenvwrapper
export WORKON_HOME=$HOME/.virtualenvs
export PROJECT_HOME=$HOME/dev
export VIRTUALENVWRAPPER_VIRTUALENV=//opt/python3.10/bin/virtualenv
export VIRTUALENVWRAPPER_PYTHON=/opt/python3.10/bin/python3
export VIRTUALENVWRAPPER_SCRIPT=/opt/python3.10/bin/virtualenvwrapper.sh

source /opt/python3.10/bin/virtualenvwrapper.sh
